@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix uco-obs: <https://ontology.unifiedcyberontology.org/uco/observable/> .
@prefix pact: <http://your-org.com/ns/pact#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .

# ----------------------------------------------------------------
# MOCK GEMARA OUTPUT
# This represents what an automated "Policy Compiler" would generate.
# It tends to be more verbose and uses specific metadata fields.
# ----------------------------------------------------------------

pact:Gemara_AC3_Shape
    a sh:NodeShape ;
    rdfs:label "Automated Check for AC-3" ;
    rdfs:comment "Generated by Gemara Compiler v1.0" ;
    
    # Gemara targets classes based on the high-level policy definition
    sh:targetClass uco-obs:File ;
    
    sh:property [
        sh:path uco-obs:owner ;
        sh:not [ sh:hasValue "root" ] ;
        sh:message "GEMARA-VIOLATION: Root ownership detected on critical file." ;
        sh:severity sh:Violation ;
    ] .

pact:Gemara_CM7_Shape
    a sh:NodeShape ;
    rdfs:label "Automated Check for CM-7" ;
    rdfs:comment "Generated by Gemara Compiler v1.0" ;
    
    sh:targetClass uco-obs:NetworkConnection ;
    
    sh:property [
        sh:path uco-obs:destinationPort ;
        # Gemara might compile a list of "Bad Ports" from a variable definition
        sh:not [ 
            sh:in ( 21 23 25 ) 
        ] ;
        sh:message "GEMARA-VIOLATION: Prohibited insecure port detected." ;
    ] .

